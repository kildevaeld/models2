package models;

import "address";

@doc("Person is the head of state")
@gotags(["json","msgpack"])
record Person {
    @constant
    id: string;
    name: string;
    @swift("desc")
    description: string;
    @cpppointer
    @gotags({"json":"ADRRESS"})
    address: models.Address;
}

record Something {
    id: string;
    createdAt: date;
}