package models;

@desc
service AddressService {
    @annotation
    get(id:string, name:string?): Address
}

@doc("This is the address")
@gotags("json")
record Address {
    @doc("This should stil be here")
    street: string?;
    city: string;
    @doc("")
    isoCode: string;

    @gopointer
    @gotags({"json":"created_at"})
    createdAt: date;
    email: [string];
    test: [string]?;

    dict: map<string,string>;
}
